{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  // "baseUrl": "./src",
  // "paths": {
  //     "@core*": [
  //         "/*"
  //     ],
  //     "@services/*": [
  //         "services/*"
  //     ]
  // },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
